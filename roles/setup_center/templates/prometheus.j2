global:
    scrape_interval: 15s
    evaluation_interval: 15s

scrape_configs:
    - job_name: "trader-exporter"
      static_configs:
          - targets: ["{{ ip_trader }}:9100"]

    - job_name: "toc-rabbitmq"
      static_configs:
          - targets: ["{{ ip_trader }}:15692"]

    - job_name: "toc-sinopac-python"
      static_configs:
          - targets: ["{{ ip_trader }}:6666"]

    - job_name: "toc-machine-trading"
      static_configs:
          - targets: ["{{ ip_trader }}:26670"]
